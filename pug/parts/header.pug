- var menuItems = ["home", "about", "hairstyles", "news", "contact"]


.header
    nav.navbar.navbar-default
        .container

            .navbar-header
                a.navbar-brand(href="#")
                    img(src="../img/logo.png" alt="Logo")
                button.navbar-toggle(type="button" data-toggle="collapse" data-target="#nav-content")
                    span.sr-only Toggle navigation
                    span.icon-bar
                    span.icon-bar
                    span.icon-bar

            .collapse.navbar-collapse#nav-content
                ul.nav.navbar-nav.navbar-right
                    each it in menuItems
                        li
                            a(href="#")= it